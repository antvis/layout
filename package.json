{
  "name": "@antv/layout",
  "version": "0.2.0-beta.3",
  "description": "graph layout algorithm",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "unpkg": "dist/layout.min.js",
  "files": [
    "es",
    "lib",
    "dist"
  ],
  "keywords": [
    "graph",
    "layout",
    "algorithm",
    "antv"
  ],
  "scripts": {
    "clean": "rimraf es lib dist",
    "lint": "eslint --cache . --ext .ts",
    "lint:fix": "eslint --fix --cache . --ext .ts",
    "build:esm": "tsc --module esnext --target es2015 --outDir ./es",
    "build:cjs": "tsc --module commonjs --target es5 --outDir ./lib",
    "build:umd": "webpack --config webpack.config.js --mode production",
    "build": "npm run clean && npm run build:cjs && npm run build:esm && npm run build:umd",
    "build:watch": "npm build:esm --w",
    "format": "prettier --config .prettierrc.js --list-different \"src/**/*.ts\"",
    "pre-commit": "tsc -p ./tsconfig.json --noEmit && lint-staged",
    "release": "release-it",
    "test": "jest",
    "coverage": "jest --coverage",
    "test-live": "DEBUG_MODE=1 jest --watch __tests__/g6/combo-concentric-test.ts",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@antv/g-webgpu": "0.5.5",
    "@dagrejs/graphlib": "2.1.4",
    "d3-force": "^2.0.1",
    "dagre-compound": "~0.0.11",
    "ml-matrix": "^6.5.0"
  },
  "devDependencies": {
    "@antv/g-webgpu-compiler": "0.5.5",
    "@antv/g6": "^4.0.3",
    "@babel/core": "*",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "*",
    "@babel/preset-typescript": "*",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/d3-force": "^2.1.0",
    "@types/graphlib": "^2.1.8",
    "@types/jest": "^25.2.1",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "babel-jest": "*",
    "babel-loader": "^8.2.2",
    "eslint": "^7.11.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "~4.0.0",
    "husky": "^7.0.4",
    "jest": "^24.0.0",
    "jest-electron": "^0.1.7",
    "jest-extended": "^0.11.2",
    "lint-staged": "^10.2.2",
    "prettier": "2.5.1",
    "release-it": "^14.0.1",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.1.0",
    "ts-loader": "^8.0.17",
    "typescript": "^4.0.3",
    "webpack": "^5.23.0",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "release-it": {
    "git": {
      "commitMessage": ":sparkles: Release ${version}"
    },
    "github": {
      "release": true
    },
    "hooks": {
      "before:init": [
        "yarn run build"
      ]
    }
  },
  "contributors": [],
  "license": "MIT",
  "homepage": "https://github.com/antvis/layout",
  "bugs": {
    "url": "https://github.com/antvis/layout/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/antvis/layout.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
