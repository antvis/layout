{
  "private": true,
  "contributors": [],
  "homepage": "https://github.com/antvis/layout",
  "bugs": {
    "url": "https://github.com/antvis/layout/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/antvis/layout.git"
  },
  "license": "MIT",
  "author": "antv",
  "scripts": {
    "lint:es": "eslint --ext .js --fix",
    "lint:ts": "tslint -c tslint.json -p packages/layout/tsconfig.json --fix",
    "lint": "npm run lint:ts && npm run lint:es",
    "lint-staged": "lint-staged",
    "build": "pnpm -r run build",
    "build:ci": "pnpm -r run build:ci",
    "watch": "pnpm --parallel -r run watch",
    "prepare": "husky install",
    "test": "jest",
    "coverage": "jest --coverage",
    "benchmarks": "run-p benchmarks:*",
    "benchmarks:forceatlas2": "node benchmarks/forceatlas2.test.js",
    "dev": "vite",
    "build:site": "vite build && cp site/enable-threads.js site/dist",
    "deploy": "gh-pages -d site/dist",
    "preview": "vite preview --port 9092"
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write"
    ],
    "*.md": [
      "markdownlint --fix",
      "case-police --fix",
      "prettier --write"
    ],
    "*.{js,jsx,tsx,ts,vue}": [
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@antv/g": "^5.18.21",
    "@antv/g-plugin-3d": "^1.9.31",
    "@antv/g-plugin-control": "^1.9.19",
    "@antv/g-webgl": "^1.9.34",
    "@antv/graphlib": "^2.0.2",
    "@babel/core": "^7.23.3",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@changesets/cli": "^2.26.2",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^21.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/d3-force": "^2.1.7",
    "@types/d3-quadtree": "^2.0.6",
    "@types/jest": "latest",
    "babel-jest": "28.1.3",
    "babel-loader": "^8.3.0",
    "benchmark": "^2.1.4",
    "cross-env": "^7.0.3",
    "case-police": "^0.5.10",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.29.0",
    "gh-pages": "^5.0.0",
    "graphology": "^0.25.4",
    "graphology-generators": "^0.11.2",
    "graphology-layout-forceatlas2": "^0.10.1",
    "graphology-types": "^0.24.7",
    "husky": "^7.0.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "29.5.0",
    "lint-staged": "^10.5.4",
    "markdownlint-cli": "^0.32.2",
    "prettier": "^2.8.8",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rollup": "^3.29.4",
    "rollup-plugin-polyfill-node": "^0.8.0",
    "rollup-plugin-visualizer": "^5.9.2",
    "seedrandom": "^3.0.5",
    "ts-jest": "^29.1.1",
    "ts-loader": "^8.4.0",
    "typescript": "^5.3.2",
    "vite": "^4.5.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
